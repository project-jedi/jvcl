package plgTest;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST ON}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'JVCL Plugin Package'}
{$RUNONLY}
{$IMPLICITBUILD ON}

requires
  vcl,
  JvCore,
  JvPluginSystem;

contains
  PluginTest in 'PluginTest.pas' {Test: TJvPlugin},
  JvPlgIntf in '..\JvPlgIntf.pas',
  ufrmPluginForm in 'ufrmPluginForm.pas' {frmPluginForm};

end.
