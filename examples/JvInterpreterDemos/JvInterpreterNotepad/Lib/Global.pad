unit Global;

var
  WorkingDir: string;

procedure InitGlobal;
begin
  WorkingDir := ExtractFilePath(Application.ExeName) + 'Lib\';
end;

procedure DoneGlobal;
begin
  { dummy }
end;


procedure DeleteEmptyLines(Ss: TStrings);
var
  i: Integer;
begin
  i := 0;
  while i <= Ss.Count - 1 do
    if Trim(Ss[i]) = '' then
      Ss.Delete(i)
    else
      i := i + 1;
end;

end.
